{
  "name": "vite-plugin-generate-dts",
  "version": "0.0.0",
  "description": "Victor Bo's Unplugin Template.",
  "license": "MIT",
  "homepage": "https://github.com/vtrbo/vite-plugin-generate-dts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vtrbo/vite-plugin-generate-dts.git"
  },
  "bugs": {
    "url": "https://github.com/vtrbo/vite-plugin-generate-dts/issues"
  },
  "keywords": [
    "vite",
    "vite-plugin"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint plugin",
    "play": "npm -C playground run dev",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest"
  },
  "dependencies": {
    "vite": "^4.4.8"
  },
  "devDependencies": {
    "@types/node": "^20.4.8",
    "@vtrbo/eslint-config": "^0.3.10",
    "bumpp": "^9.1.1",
    "chalk": "^5.3.0",
    "eslint": "^8.46.0",
    "esno": "^0.17.0",
    "fast-glob": "^3.3.1",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0"
  }
}
